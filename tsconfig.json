{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "#*": ["./src/*"],
      "#lib/*": ["lib/*"],
      "#components/*": ["./src/components/*"],
      "#hooks/*": ["./src/hooks/*"],
      "#public/*": ["public/*"],
      "#utils/*": ["./src/utils/*"],
      "#context/*": ["./src/context/*"],
      "#constants/*": ["./src/constants/*"],
      "#apis/*": ["./src/apis/*"],
      "#store/*": ["./src/store/*"],
      "#types/*": ["./src/types/*"],
      "#queries/*": ["./src/queries/*"],
      "style/*": ["./src/styles/*"],
      "schema/*": ["./src/schema/*"]
    },
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "middleware.js"
  ],
  "exclude": ["node_modules"]
}
